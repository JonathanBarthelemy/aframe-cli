{
  "name": "aframe-cli",
  "description": "A command-line tool for building A-Frame scenes.",
  "author": "A-Frame authors",
  "version": "0.1.1",
  "main": "index.js",
  "bin": {
    "aframe": "index.js"
  },
  "license": "MIT",
  "repository": "cvan/aframe-cli",
  "scripts": {
    "start": "npm run cli",
    "cli": "node index.js",
    "release": "npm run release-patch",
    "release-patch": "npm version patch && npm whoami && npm publish && git push --tags",
    "release-all": "npm run release-templates && npm run publish",
    "release-templates": "node tools/publish-all-templates.js"
  },
  "dependencies": {
    "auto-reload-brunch": "^2",
    "babel-brunch": "^6.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "brunch": "^2",
    "chalk": "^1.1.3",
    "clean-css-brunch": "^2",
    "clipboardy": "^1.1.2",
    "commander": "^2.9.0",
    "fs-extra": "^3.0.1",
    "loggy": "^1.0.2",
    "oao": "^0.8.2",
    "uglify-js-brunch": "^2"
  },
  "devDependencies": {
    "graph-data-structure": "^0.8.0",
    "semver": "^5.3.0"
  }
}
